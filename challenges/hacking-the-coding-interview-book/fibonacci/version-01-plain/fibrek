 */
var fibrek = (function fibrek() {

    function fibrek(n) {

        // could be written in if (n < 2) return n
        if (n === 0) return 0;
        if (n === 1) return 1;

        if (n > 1) {
            return fibrek(n - 2) + fibrek(n - 1);
        }
    }

    return {
        fibrek: fibrek
    }

});

/*
* Big O is O(n^2) as there are 2 functions are being called for each n.
*/